{
  "buildCommand": "cd ../.. && pnpm build --filter=robin-noguier",
  "outputDirectory": "dist",
  "installCommand": "cd ../.. && ./dev_setup.sh",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
